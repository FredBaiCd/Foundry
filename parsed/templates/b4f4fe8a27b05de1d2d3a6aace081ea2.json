{"components/b2b/quick-order-pad-product":"<div data-product-priceValue=\"{{product.price.without_tax.value}}{{product.price.with_tax.value}}\" {{#if product.options}}has-options=\"true\"{{/if}}>\r\n\t<div class=\"productInfo-name\">{{product.title}}</div>\r\n\t<div class=\"productInfo-price\"><span data-price>{{product.price.without_tax.formatted}}{{product.price.with_tax.formatted}}</span></div>\r\n\t{{#any product.options type='Configurable_Entry_Text'}}\r\n\t<div class=\"productInfo-options\">\r\n\t\t{{#each product.options}}\r\n\t\t{{#if type '===' 'Configurable_Entry_Text'}}\r\n\t\t{{{dynamicComponent 'components/products/options'}}}\r\n\t\t{{/if}}\r\n\t\t{{/each}}\r\n\t</div>\r\n\t{{/any}}\r\n</div>\n","components/products/options/date":"<div class=\"form-field form-field-options form-field-date{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"date\">\n  <label class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][year]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.year'}}</option>\n          {{#enumerate earliest_year latest_year}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.year this}}selected=\"selected\"{{/equals}}>{{this}}</option>\n          {{/enumerate}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][month]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#enumerate 1 12}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.month this}}selected=\"selected\"{{/equals}}>{{getShortMonth this}}</option>\n          {{/enumerate}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][day]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#enumerate 1 31}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.day this}}selected=\"selected\"{{/equals}}>{{this}}</option>\n          {{/enumerate}}\n        </select>\n        </span>\n    </span>\n\n  </label>\n</div>\n","components/products/options/input-checkbox":"<div class=\"form-field form-field-options form-field-inline form-field-checkbox{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-checkbox\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text\">{{label}}</span>\n    </label>\n  </div>\n</div>\n","components/products/options/input-numbers":"<div class=\"form-field form-field-options form-field-inline form-field-number{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-number\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\" id=\"attribute-{{id}}\" name=\"attribute[{{id}}]\" type=\"number\" value=\"{{prefill}}\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","components/products/options/input-text":"<div class=\"form-field form-field-options form-field-inline form-field-text{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-text\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","components/products/options/product-list":"\n<div class=\"form-field form-field-options form-field-product-list{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"product-list\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#unless values.0.image}}\n      {{!-- same as radio buttons if no image set --}}\n      {{#each values}}\n        <label class=\"form-label form-field-radio product-picklist-no-image\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            data-product-id=\"{{data}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{label}}</span>\n        </label>\n      {{/each}}\n      <label class=\"form-label form-field-radio product-picklist-no-image\">\n        <input\n          class=\"form-input form-radio product-picklist-radio\"\n          name=\"attribute[{{id}}]\"\n          type=\"radio\" value=\"0\"\n          data-product-id=\"{{data}}\"\n          {{#if defaultValue '==' 0}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{lang 'product.none'}}</span>\n      </label>\n    {{else}}\n      {{#each values}}\n        <label class=\"form-label form-field-radio product-picklist-item\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\" value=\"{{id}}\"\n            data-product-id=\"{{data}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"product-picklist-item-body\">\n            <span class=\"product-picklist-image-wrap\">\n              <img class=\"product-picklist-image\" src=\"{{getImage image 'product_thumbnail'}}\" alt=\"{{image.alt}}\">\n            </span>\n            <img class=\"hover-picklist-image\" src=\"{{getImage image 'product_thumbnail'}}\" alt=\"{{image.alt}}\">\n            <span class=\"product-picklist-label-text form-label-text\">{{label}}</span>\n          </span>\n        </label>\n      {{/each}}\n      <label class=\"form-label form-field-radio product-picklist-item\">\n        <input\n          class=\"form-input form-radio product-picklist-radio\"\n          name=\"attribute[{{id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          data-product-id=\"{{data}}\"\n          {{#if defaultValue '==' 0}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"product-picklist-item-body picklist-none\">\n          <span class=\"product-picklist-label-text form-label-text\">{{lang 'product.none'}}</span>\n        </span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n","components/products/options/set-radio":"<div class=\"form-field form-field-options form-field-radio form-field-inline {{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-radio\">\n  <div class=\"form-field-title product-option-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-radio\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","components/products/options/set-select":"<div class=\"form-field form-field-options form-field-inline form-field-select{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-select\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <div class=\"form-select-wrapper\">\n        <select\n          class=\"form-input form-select\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\">{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{id}}\" {{#if selected}}selected{{/if}} data-product-attribute-value=\"{{id}}\">{{label}}</option>\n          {{/each}}\n        </select>\n      </div>\n    </span>\n  </label>\n</div>\n","components/products/options/set-rectangle":"<div class=\"form-field form-field-options form-field-inline form-field-rectangle{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-rectangle\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"rectangle-text form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","components/products/options/swatch":"<div class=\"form-field form-field-options form-field-inline form-field-swatch{{#if required}} form-required{{/if}}\"\n  data-swatch-selector\n  data-product-attribute=\"swatch\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"swatch-wrap\" data-swatch-value=\"{{label}}\" data-product-attribute-value=\"{{id}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n\n        <input\n          class=\"form-input swatch-radio\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          name=\"attribute[{{../id}}]\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n\n        <span class=\"swatch {{#if data.[1]}}two-colors{{/if}}{{#if data.[2]}} three-colors{{/if}}\">\n          {{#if pattern}}\n            <span class=\"swatch-color swatch-pattern\" style=\"background-image: url('{{getImage image 'product_thumbnail'}}');\"></span>\n          {{/if}}\n\n          {{#if data.[0]}}\n            <span class=\"swatch-color\" style=\"background-color: #{{data.[0]}}\"></span>\n          {{/if}}\n\n          {{#if data.[1]}}\n            <span class=\"swatch-color secondary\" style=\"background-color: #{{data.[1]}}\"></span>\n          {{/if}}\n\n          {{#if data.[2]}}\n            <span class=\"swatch-color tertiary\" style=\"background-color: #{{data.[2]}}\"></span>\n          {{/if}}\n        </span>\n\n        {{#if pattern}}\n          <span class=\"swatch-pattern-expanded\">\n            <img class=\"swatch-pattern-image\" src=\"{{getImage image 'product_item'}}\">\n          </span>\n        {{/if}}\n\n      </label>\n    {{/each}}\n  </div>\n\n  <span class=\"swatch-value\" data-swatch-value>{{lang 'forms.swatches_none_selected'}}</span>\n\n</div>\n","components/products/options/input-file":"<div class=\"form-field form-field-options form-field-file{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-file\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'product.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n    {{#if file_types}}\n      <span class=\"form-file-types\">{{lang 'forms.validation.accepted_file_types' file_types=file_types}}</span>\n    {{else}}\n      <span class=\"form-file-types\">{{lang 'forms.validation.all_accepted_file_types'}}</span>\n    {{/if}}\n  </label>\n</div>\n","components/products/options/textarea":"<div class=\"form-field form-field-options form-field-textarea{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"textarea\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n"}
