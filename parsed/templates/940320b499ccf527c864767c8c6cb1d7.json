{"components/b2b/product-view-data":"<div class=\"productView\" data-product-info\r\n    data-product-image=\"{{#if product.main_image}}{{getImage product.main_image 'shopping_list_size' (cdn theme_settings.default_image_product)}}{{/if}}\"  \r\n    data-product-title=\"{{product.title}}\"\r\n    data-product-sku=\"{{#if product.sku}}{{product.sku}}{{/if}}\"\r\n    data-product-price=\"{{product.price.without_tax.formatted}}{{product.price.with_tax.formatted}}\"\r\n    data-product-priceValue=\"{{product.price.without_tax.value}}{{product.price.with_tax.value}}\"\r\n    data-product-url=\"{{product.url}}\"\r\n    {{#if product.options}}has-options=\"true\"{{/if}}\r\n    data-product-options=\"{{JSONstringify product.options}}\"\r\n></div>\r\n"}
