{"components/footer":"<footer class=\"main-footer\">\n  <div class=\"container\">\n\n    <section class=\"footer-left\">\n\n      {{#if social_media.length}}\n        <div class=\"footer-navigation\">\n          <h3 class=\"footer-section-title\">{{lang 'footer.headings.social_media'}}</h3>\n          <ul>\n            {{#each social_media}}\n              <li class=\"footer-navigation-list-item\">\n                {{#compare display_name 'google+' operator='==='}}\n                  <a href=\"{{url}}\" class=\"google\" target=\"_blank\">\n                    <i class=\"icon-google\"></i> Google+\n                  </a>\n                {{else}}\n                  <a href=\"{{url}}\" class=\"{{display_name}}\" target=\"_blank\">\n                    <i class=\"icon-{{display_name}}\"></i> {{display_name}}\n                  </a>\n                {{/compare}}\n              </li>\n            {{/each}}\n          </ul>\n        </div>\n      {{/if}}\n\n      {{#if theme_settings.footer_menu '===' 'footer-show-pages' }}\n        {{#if pages.length}}\n          {{> components/footer-navigation nav_items=pages}}\n        {{/if}}\n      {{else if theme_settings.footer_menu '===' 'footer-show-categories'}}\n        {{> components/footer-navigation nav_items=categories view_all=false}}\n      {{else}}\n        {{> components/footer-navigation nav_items=shop_by_brand view_all=true}}\n      {{/if}}\n\n      <div class=\"footer-navigation\">\n        <h3 class=\"footer-section-title\">{{lang 'footer.headings.user_links'}}</h3>\n        <ul>\n          {{#if settings.gift_certificates_enabled}}\n            <li class=\"footer-navigation-list-item\">\n              <a href=\"{{urls.gift_certificate.purchase}}\">{{lang 'footer.gift_certificate'}}</a>\n            </li>\n          {{/if}}\n\n          {{#if settings.show_wishlist}}\n            {{#if customer}}\n              <li class=\"footer-navigation-list-item\">\n                <a href=\"{{urls.account.wishlists.all}}\">{{lang 'footer.wishlist'}}</a>\n              </li>\n            {{/if}}\n          {{/if}}\n\n          {{#if settings.account_creation_enabled}}\n            {{#if customer}}\n              <li class=\"footer-navigation-list-item\">\n                <a href=\"{{urls.account.index}}\">{{lang 'footer.your_account'}}</a>\n              </li>\n              <li class=\"footer-navigation-list-item\">\n                <a href=\"{{urls.auth.logout}}\">{{lang 'footer.log_out'}}</a>\n              </li>\n            {{else}}\n              <li class=\"footer-navigation-list-item\">\n                <a href=\"{{urls.auth.login}}\">{{lang 'footer.log_in'}}</a>\n              </li>\n              <li class=\"footer-navigation-list-item\">\n                <a href=\"{{urls.auth.create_account}}\">{{lang 'footer.sign_up'}}</a>\n              </li>\n            {{/if}}\n          {{/if}}\n        </ul>\n      </div>\n\n    </section>\n\n    <div class=\"footer-currency-selector\">\n      <p class=\"footer-currency-selector-title show-for-sr\">{{lang 'footer.headings.select_currency'}}</p>\n      {{> components/currency-selector is-mobile=true}}\n    </div>\n\n    <section class=\"footer-right\">\n\n      {{#if settings.show_newsletter_box}}\n        <div class=\"footer-newsletter\">\n          <h3 class=\"footer-section-title\">{{lang 'newsletter.heading'}}</h3>\n          <form class=\"footer-newsletter-form form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n            <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n            <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n            <input type=\"hidden\" name=\"check\" value=\"1\">\n            <label class=\"form-label show-for-sr\" for=\"nl_email\">{{lang 'newsletter.email'}}</label>\n            <input class=\"newsletter-signup-email form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email'}}\">\n            <div class=\"newsletter-signup-submit\">\n              <input class=\"submit button\" type=\"submit\" value=\"{{lang 'newsletter.subscribe'}}\">\n            </div>\n          </form>\n        </div>\n      {{/if}}\n        <div id=\"HouzzContainer\">\n            <img src=\"{{cdn \"img/houzz2017.jpg\"}}\">\n        </div>\n      <div class=\"footer-payment-icons\">\n        <ul class=\"footer-payment-icons-list\">\n          {{#if theme_settings.payment_visa}}\n            <li><i class=\"icon-visa\"></i></li>\n          {{/if}}\n\n          {{#if theme_settings.payment_mastercard}}\n            <li><i class=\"icon-mastercard\"></i></li>\n          {{/if}}\n\n          {{#if theme_settings.payment_american_express}}\n            <li><i class=\"icon-americanexpress\"></i></li>\n          {{/if}}\n\n          {{#if theme_settings.payment_discover}}\n            <li><i class=\"icon-discover\"></i></li>\n          {{/if}}\n\n          {{#if theme_settings.payment_paypal}}\n            <li><i class=\"icon-paypal\"></i></li>\n          {{/if}}\n\n          {{#if theme_settings.payment_apple_pay}}\n            <li><i class=\"icon-apple-pay\"></i></li>\n          {{/if}}\n        </ul>\n      </div>\n\n      {{#if theme_settings.theme_credits}}\n        <div class=\"footer-attribution\">\n          <p>Foundry {{lang 'footer.credits.theme_by'}} <a href=\"http://pixelunion.net\" target=\"_blank\" rel=\"nofollow\">Pixel Union</a>, {{lang 'footer.credits.powered_by'}} <a href=\"http://www.bigcommerce.com/\" target=\"_blank\" rel=\"nofollow\">BigCommerce</a></p>\n        </div>\n      {{/if}}\n\n      {{#if theme_settings.theme_copyright}}\n        <div class=\"footer-copyright\">\n          <p>{{lang 'footer.credits.copyright' name=settings.store_name}} &copy; <script>document.write(new Date().getFullYear())</script></p>\n        </div>\n      {{/if}}\n    </section>\n\n  </div>\n\n  <div class=\"footer-contact\">\n    {{#if settings.address.length}}\n      <p>\n        {{ settings.address }}\n      </p>\n    {{/if}}\n\n    {{#if settings.phone_number.length}}\n      <p>\n        {{ settings.phone_number }}\n      </p>\n    {{/if}}\n  </div>\n</footer>\n<!-- temp snippets for AP Development \n  <script>if(window.location.href.indexOf(\"gallery\")>-1){console.log('gallery page detected...load gallery scripts');var head=document.getElementsByTagName('head')[0];var gjs=document.createElement('script');gjs.type='text/javascript';gjs.src='/content/bc-gallery/js/bc-gallery.js';head.appendChild(gjs);}</script>\n<script type=\"text/javascript\" src=\"/content/bc-gallery/js/popup.js\"></script>  -->\n\n<script src=\"https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js\"></script>\n<script src=\"https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js\"></script>\n<script type=\"text/javascript\" src=\"/content/custom/custom-scripts.js\"></script>\n<!-- BC Gallery files -->\n<script type=\"text/javascript\" src=\"/content/bc-gallery/js/bc-gallery.js\"></script>\n\n<!-- tooltip color/image -->\n<style> span.tooltip-icon { background: url(//cdn6.bigcommerce.com/s-d8f8g8yrzc/product_images/uploaded_images/question-mark-gray.png) no-repeat;}</style>\n\n<div style=\"display:none;\" id=\"tooltip-list\">\n\n    <!-- enter tooltip data/content below -->\n     <div class=\"tooltip-item\">\n        <div class=\"tooltip-title\">Gate Width</div>\n        <div class=\"tooltip-content\"><img src=\"https://cdn6.bigcommerce.com/s-d8f8g8yrzc/product_images/uploaded_images/width-and-height-image.jpg?t=1490820706&_ga=1.20280696.1930763316.1485274129\" /></div>\n    </div>\n\n    <div class=\"tooltip-item\">\n        <div class=\"tooltip-title\">Gate Height</div>\n        <div class=\"tooltip-content\"><img src=\"https://cdn6.bigcommerce.com/s-d8f8g8yrzc/product_images/uploaded_images/width-and-height-image.jpg?t=1490820706&_ga=1.20280696.1930763316.1485274129\" /></div>\n    </div>\n    \n    <div class=\"tooltip-item\">\n         <div class=\"tooltip-title\">Frame Thickness</div>\n        <div class=\"tooltip-content\"><img src=\"https://cdn6.bigcommerce.com/s-d8f8g8yrzc/product_images/uploaded_images/frame-thickness.jpg?t=1492454077&_ga=1.140993590.580600017.1487635692\" /></div>\n    </div>\n    \n    <div class=\"tooltip-item\">\n         <div class=\"tooltip-title\">Frame Thickness (Upgrade 2-1/8\" = $360)</div>\n        <div class=\"tooltip-content\"><img src=\"https://cdn6.bigcommerce.com/s-d8f8g8yrzc/product_images/uploaded_images/frame-thickness.jpg?t=1492454077&_ga=1.140993590.580600017.1487635692\" /></div>\n    </div>\n    <div class=\"tooltip-item\">\n        <div class=\"tooltip-title\">Stain Options (Add $140)</div>\n        <div class=\"tooltip-content pdf\">stain-options.pdf</div>\n    </div>\n    \n    <div class=\"tooltip-item\">\n        <div class=\"tooltip-title\">Stain Options (Add $280)</div>\n        <div class=\"tooltip-content pdf\">stain-options.pdf</div>\n    </div>\n    \n    <div class=\"tooltip-item\">\n        <div class=\"tooltip-title\">Stain Options (Add $420)</div>\n        <div class=\"tooltip-content pdf\">stain-options.pdf</div>\n    </div>\n    \n    <div class=\"tooltip-item\">\n        <div class=\"tooltip-title\">Hardware Bundles (incl prep and jambs)</div>\n         <div class=\"tooltip-content\"><img src=\"https://cdn6.bigcommerce.com/s-d8f8g8yrzc/product_images/uploaded_images/hardware-bundle-options-single-swing-updated.jpg?t=1493246076&_ga=1.20225400.1930763316.1485274129\" /></div>\n    </div>\n    <div class=\"tooltip-item\">\n        <div class=\"tooltip-title\">Hardware Bundle Option</div>\n         <div class=\"tooltip-content\"><img src=\"https://cdn6.bigcommerce.com/s-d8f8g8yrzc/product_images/uploaded_images/hardware-bundle-options-driveway.jpg?t=1494450515&_ga=2.17892089.1729182210.1494450559-1930763316.1485274129\" /></div>\n    </div>\n    \n    <div class=\"tooltip-item\">\n        <div class=\"tooltip-title\">Hardware Option</div>\n         <di